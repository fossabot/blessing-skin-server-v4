{
  "name": "blessing-skin-server",
  "version": "4.0.0-alpha.0",
  "description": "A web application brings your custom skins back in offline Minecraft servers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/printempw/blessing-skin-server"
  },
  "bugs": {
    "url": "https://github.com/printempw/blessing-skin-server/issues/new"
  },
  "author": "printempw <h@prinzeugen.net>",
  "license": "GPL-3.0",
  "contributors": [
    "Pig Fang <g-plane@hotmail.com>"
  ],
  "private": true,
  "scripts": {
    "init": "webpack --progress --config webpack.dev.config.js",
    "dev": "webpack-dev-server --hot --config resources/assets/build/webpack.dev.conf.js",
    "build": "webpack --progress --hide-modules --config resources/assets/build/webpack.prod.conf.js",
    "unit": "karma start resources/assets/tests/karma.conf.js --no-auto-watch --single-run",
    "unit:watch": "karma start resources/assets/tests/karma.conf.js --auto-watch --no-single-run",
    "test": "yarn unit",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' --ext .ts,.vue resources/assets",
    "prettier": "prettier --config --write 'resources/assets/**/*.{vue,ts,less,gql,graphql}'",
    "lint": "yarn eslint --fix && yarn prettier --loglevel warn",
    "lint:check": "yarn eslint && yarn prettier --list-different"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link": "^1.0.7",
    "apollo-link-http": "^1.3.2",
    "axios": "^0.17.1",
    "core-js": "^2.5.3",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "iview": "^2.9.1",
    "moment": "^2.20.1",
    "moment-precise-range-plugin": "^1.3.0",
    "vee-validate": "^2.0.3",
    "vue": "^2.5.2",
    "vue-apollo": "^3.0.0-alpha.3",
    "vue-i18n": "^7.4.0",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.2.1",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.29",
    "@types/zxcvbn": "^4.4.0",
    "@vue/test-utils": "^1.0.0-beta.11",
    "autoprefixer-loader": "^3.2.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-typescript": "^0.8.1",
    "eslint-plugin-vue": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "iview-loader": "^1.0.0",
    "karma": "2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-nyan-reporter": "^0.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^2.0.9",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mocha": "^5.0.0",
    "moxios": "^0.4.0",
    "node-notifier": "^5.2.1",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.1",
    "typescript-eslint-parser": "^13.0.0",
    "url-loader": "^0.6.2",
    "vue-hot-reload-api": "^2.2.4",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^14.1.1",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.1",
    "yaml-loader": "^0.5.0"
  }
}
