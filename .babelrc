{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        "syntax-dynamic-import",
        [
            "transform-runtime",
            {
                "polyfill": false,
                "regenerator": true
            }
        ]
    ]
}
